<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
    </div>
    </div>
    <% if (user){ %>
      <div>
        <form id="logout" method="POST" action="/logout" style="font-size: 1.05em">
          <span>Logged In As: <%=user.email%></span>
          <button class="btn btn-secondary" type="submit" style="margin-left: 0.7em" >Logout</button>
        </form>
      </div>
      <div>
      </div>
      <% } else{ %>
        <div class="text-right">
          <a class="btn btn-primary" href="/login">Login</a>  
            <a class="btn btn-secondary" href= "/register">Register</a>
        </div>
        <div>
        </div>
        <% } %>
    </div>
      </nav>
      </header>